<div class="side-top">
  <div class="user-name">
    <%= link_to current_user.name, edit_user_path(current_user) %>
  </div>
  <div class="create-room">
    <%= link_to "チャットを作成する", new_room_path %>
  </div>
</div>
<div class="side-main">
  <% Room.all.each do |room| %>
    <div class="chat-room">
      <div class="chat-room-name">
        <a href="#"><%= room.name %></a>
      </div>
    </div>
  <% end %>
</div>